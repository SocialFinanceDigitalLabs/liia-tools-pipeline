<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="Message" type="messagetype"/>

  <xs:complexType name="messagetype">
    <xs:sequence>
      <xs:element name="Header" type="headertype" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Persons" type="personstype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

  <xs:complexType name="headertype">
    <xs:sequence>
      <xs:element name="CollectionDetails" type="collectiondetailstype" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Source" type="sourcetype" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="collectiondetailstype">
    <xs:sequence>
      <xs:element name="Collection" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="SEN2"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="Year" type="xs:gYear" minOccurs="0" maxOccurs="1"/>
      <xs:element name="ReferenceDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="sourcetype">
    <xs:sequence>
      <xs:element name="SourceLevel" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="L"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="LEA" type = "exportedandleatype" minOccurs="0" maxOccurs="1"/>
      <xs:element name="SoftwareCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="Release" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <xs:element name="SerialNo" minOccurs="0" maxOccurs="1">
        <xs:simpleType>
          <xs:restriction base="xs:string" >
            <xs:pattern value="\d{3}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="DateTime" type="xs:dateTime" minOccurs="0" maxOccurs="1"/>
      <xs:element name="DMO" type="yesnorecruittype" minOccurs="1" maxOccurs="1"/>
      <xs:element name="DCO" type="yesnorecruittype" minOccurs="1" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

<!-- Module 1: Person details <Person> -->
  <xs:complexType name="personstype">
    <xs:sequence>
      <xs:element name="Person" type="persondetailstype" minOccurs="1" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>

<xs:complexType name="persondetailstype">
<xs:sequence>
      <!-- Item 1.1 in guide for LAs -->
      <xs:element name="Surname" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <!-- Item 1.2 in guide for LAs -->
      <xs:element name="Forename" type="xs:string" minOccurs="1" maxOccurs="1"/>
      <!-- Item 1.3 in guide for LAs -->
      <xs:element name="PersonBirthDate" type="xs:date" minOccurs="1" maxOccurs="1"/>
      <!-- Item 1.4 in guide for LAs -->
      <xs:element name="Sex" type="sextype" minOccurs="1" maxOccurs="1"/>
      <!-- Item 1.5 in guide for LAs -->
      <xs:element name="Ethnicity" type="ethnicitytype" minOccurs="1" maxOccurs="1"/>
      <!-- Item 1.6 in guide for LAs -->
      <xs:element name="PostCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
      <!-- Item 1.7a in guide for LAs -->
      <xs:element name="UPN" type="upntype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 1.7b in guide for LAs -->
      <xs:element name="UniqueLearnerNumber" type="uniquelearnernumbertype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 1.7c in guide for LAs -->
      <xs:element name="UPNunknown" type="upnunknowntype" minOccurs="0" maxOccurs="1"/>
      <!-- Need line below so we can have section below! -->
      <xs:element name="Requests" type="requeststype" minOccurs="0" maxOccurs="unbounded"/>
</xs:sequence>
</xs:complexType>

<!-- Requests section has RequestOutcome, Assessment, and ActivePlans sections -->
<!-- Assessment section has AssessmentOutcome and NamedPlan sections... -->
<!-- ... where NamedPlan section has StartDate, PlanDetail, PB, CeaseDate, and CeaseReason bits -->
<!-- ... where PlanDetail section has URN and PlacementRank bits -->
<!-- ActivePlans section has PlacementDetail and SENneed sections... -->
<!-- ... where PlacementDetail section has URN, PlacementRank and EntryDate bits-->
<!-- ... where SENneed section has SENtype and SENtypeRank bits -->

<!-- Module 2: Requests for EHC needs assessment <Requests> -->
<!-- Requests section has RequestOutcome, Assessment, and ActivePlans sections -->
<xs:complexType name="requeststype">
<xs:sequence>
      <!-- Item 2.1 in guide for LAs -->
      <!-- LA guide says "Not required if request outcome is historical (H)." Does this affect simpletype for ReceivedDate? Do we need to add further down?? -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="ReceivedDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 2.2 in guide for LAs -->
      <!-- LA guide says "Not required if request outcome is historical (H)." Does this affect simpletype for RYA? Do we need to do something else further down?? -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="RYA" type="yesnotype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 2.3 in guide for LAs -->
      <!-- No date is required in certain cases - see guide for LAs for more info -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="RequestOutcomeDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 2.4 in guide for LAs -->
      <!-- Guide for LAs says "When A, W or H is selected, no further information is required in this module" - that is, we can stop after ReceivedDate, RYA, RequestOutcomeDate, and RequestOutcome have been provided? Should we add something in the code about this? -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="RequestOutcome" type="requestoutcometype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 2.5 in guide for LAs -->
      <!-- Guide for LAs mentions looking at Annex C - Mediation for further information -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="RequestMediation" type="requestmedtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 2.6 in guide for LAs -->
      <!-- Guide for LAs mentions looking at Annex C - Mediation for further information -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="RequestTribunal" type="requesttribtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 2.7 in guide for LAs -->
      <!-- Guide for LAs says "No further information is subsequently required from the exporting local authority." i.e. no more information for module 3 and onwards is required? Should we incorporate something into the code? -->
<!-- XSD CHECKER CAME UP WITH AN ERROR IN LINE BELOW -->
      <xs:element name="Exported" type="exportedandleatype" minOccurs="0" maxOccurs="1"/>
      <!-- Need line below so we can have section below! -->
      <xs:element name="Assessment" type="assessmenttype" minOccurs="0" maxOccurs="1"/>
      <!-- Need line below so we can have section below! -->
      <xs:element name="ActivePlans" type="activeplanstype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- Module 3: EHC needs assessments <Assessment> -->
<!-- Assessment section has AssessmentOutcome and NamedPlan sections... -->
<xs:complexType name="assessmenttype">
<xs:sequence>
      <!-- Item 3.1 in guide for LAs -->
      <!-- Guide for LAs mentions "Where A or W is selected, no further information is required in this or subsequent modules." Could we incorporate this into our code? -->
      <xs:element name="AssessmentOutcome" type="assessmentoutcometype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 3.2 in guide for LAs -->
      <!-- Guide for LAs says "The assessment outcome date is required where 3.1 is equal to ‘Y’ or ‘N’" - Can we add something to the code about this? -->
      <!-- Guide for LAs says this will show either "Date on which EHC plan was issued" or "Date on which person was notified of decision not to issue a plan" -->
      <xs:element name="AssessmentOutcomeDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 3.3 in guide for LAs -->
      <!-- Guide for LAs says "For further information see Annex C – Mediation." -->
      <xs:element name="AssessmentMediation" type="assessmedtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 3.4 in guide for LAs -->
      <!-- Guide for LAs says "For further information see Annex C – Mediation." -->
      <xs:element name="AssessmentTribunal" type="assesstribtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 3.5 in guide for LAs -->
      <!-- Guide for LAs says "For further information see Annex C – Mediation." -->
      <xs:element name="OtherMediation" type="othermedtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 3.6 in guide for LAs -->
      <!-- Guide for LAs says "For further information see Annex C – Mediation." -->
      <xs:element name="OtherTribunal" type="othertribtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 3.7 in guide for LAs -->
      <xs:element name="Week20" type="week20type" minOccurs="0" maxOccurs="1"/>
      <!-- Need line below so we can have section below! -->
      <xs:element name="NamedPlan" type="namedplantype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- Module 4: Named plan <NamedPlan> -->
<!-- ... where NamedPlan section has StartDate, PlanDetail, PB, CeaseDate, and CeaseReason bits -->
<xs:complexType name="namedplantype">
<xs:sequence>
      <!-- Item 4.1 in guide for LAs -->
      <xs:element name="StartDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7 in guide below has a subset mentioned below -->
      <xs:element name="PlanDetail" type="plandetailtype" minOccurs="0" maxOccurs="2"/>
      <!-- Item 4.2 in guide for LAs -->
      <xs:element name="PlanRes" type="planrestype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.3 in guide for LAs -->
      <xs:element name="PlanWBP" type="wbptype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.4a in guide for LAs -->
      <xs:element name="PB" type="pbtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.4b in guide for LAs -->
      <xs:element name="OA" type="oatype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.4c in guide for LAs -->
      <xs:element name="DP" type="dptype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.5 in guide for LAs -->
      <xs:element name="CeaseDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.6 in guide for LAs -->
      <xs:element name="CeaseReason" type="ceasereasontype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- ... where PlanDetail section has URN and PlacementRank bits -->
<xs:complexType name="plandetailtype">
<xs:sequence>
      <!-- Item 4.7a in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Guide for LAs says "Please complete only one of 5.7a to 5.7c." -->
      <!-- Item 5.7a in guide for LAs -->
      <xs:element name="URN" type="urntype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7b in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Item 5.7b in guide for LAs -->
      <xs:element name="UKPRN" type="ukprntype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7c in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Item 5.7c in guide for LAs -->
      <xs:element name="SENSetting" type="sensettingtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7c(i) in guide for LAs -->
      <!-- Item 5.7c(i) in guide for LAs -->
      <!-- Item 5.7c(i) - Please note guide for LAs says "Where 5.5c is ‘Not in education – other’ please provide further information, e.g.,permanently excluded." - Should this be another simpletype??-->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <xs:element name="SENSettingOther" type="nonEmptyString" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7d in guide for LAs -->
      <!-- Item 5.7d in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Guide for LAs says "Placement rank (primary or secondary placement) should also be completed (item 5.5d) for each placement." -->
      <xs:element name="PlacementRank" type="placementranktype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7e in guide for LAs -->
      <!-- Item 5.7g in guide for LAs -->
      <xs:element name="SENunitIndicator" type="yesnotype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7f in guide for LAs -->
      <!-- Item 5.7h in guide for LAs -->
      <xs:element name="ResourcedProvisionIndicator" type="yesnotype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- Module 5: Placements <ActivePlans> -->
<!-- ActivePlans section has PlacementDetail and SENneed sections... -->
<xs:complexType name="activeplanstype">
<xs:sequence>
      <!-- Item 5.1 in guide for LAs -->
      <xs:element name="TransferLA" type="exportedandleatype" minOccurs="0" maxOccurs="1"/>
      <xs:element name="PlacementDetail" type="placementdetailtype" minOccurs="0" maxOccurs="unbounded"/>
      <!-- Item 5.8 in guide for LAs (has subsets mentioned below)-->
      <!-- Previous conversations that up to 2 SEN need types can be mentioned in the return -->
      <xs:element name="SENneed" type="senneedtype" minOccurs="0" maxOccurs="2"/>
      <!-- Item 5.4 in guide for LAs -->
      <!-- Following is a new voluntary data item added -->
      <xs:element name="ReviewMeeting" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 5.5 in guide for LAs -->
      <!-- Following is a new voluntary data item added -->
      <xs:element name="ReviewOutcome" type="reviewoutcometype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 5.6 in guide for LAs -->
      <xs:element name="LastReview" type="xs:date" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- ... where PlacementDetail section has URN, PlacementRank and EntryDate bits-->
<xs:complexType name="placementdetailtype">
<xs:sequence>
      <!-- Item 4.7a in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Guide for LAs says "Please complete only one of 5.7a to 5.7c." -->
      <!-- Item 5.7a in guide for LAs -->
      <xs:element name="URN" type="urntype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7b in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Item 5.7b in guide for LAs -->
      <xs:element name="UKPRN" type="ukprntype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7c in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Item 5.7c in guide for LAs -->
      <xs:element name="SENSetting" type="sensettingtype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7c(i) in guide for LAs -->
      <!-- Item 5.7c(i) in guide for LAs -->
      <!-- Item 5.7c(i) - Please note guide for LAs says "Where 5.5c is ‘Not in education – other’ please provide further information, e.g.,permanently excluded." - Should this be another simpletype??-->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <xs:element name="SENSettingOther" type="nonEmptyString" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7d in guide for LAs -->
      <!-- Item 5.7d in guide for LAs -->
      <!-- Guide for LAs mentions only completing one of 4.7a to 4.7c. Item 4.7d (Placement rank) should also be completed -->
      <!-- Guide for LAs says "Placement rank (primary or secondary placement) should also be completed (item 5.5d) for each placement." -->
      <xs:element name="PlacementRank" type="placementranktype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 5.7e in guide for LAs -->
      <xs:element name="EntryDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 5.7f in guide for LAs -->
      <xs:element name="LeavingDate" type="xs:date" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7e in guide for LAs -->
      <!-- Item 5.7g in guide for LAs -->
      <xs:element name="SENunitIndicator" type="yesnotype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 4.7f in guide for LAs -->
      <!-- Item 5.7h in guide for LAs -->
      <xs:element name="ResourcedProvisionIndicator" type="yesnotype" minOccurs="0" maxOccurs="1"/>
<!-- Guide for LAs says "Data items ‘residential settings’ and ‘work based learning activity’ have been moved from <ActivePlans> into the <PlacementDetail> module in the XML structure. Please see paragraphs 2.5 and 3.2.2." as per technical spec, however in guide for LAs it still seems to be under ActivePlans (for former please see page 11 and for latter please see page 31)?? -->
      <!-- Item 5.2 in guide for LAs -->
      <xs:element name="Res" type="planrestype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 5.3 in guide for LAs -->
      <xs:element name="WBP" type="wbptype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- ... where SENneed section has SENtype and SENtypeRank bits -->
<xs:complexType name="senneedtype">
<xs:sequence>
      <!-- Item 5.8a in guide for LAs -->
      <xs:element name="SENtype" type="sentype" minOccurs="0" maxOccurs="1"/>
      <!-- Item 5.8b in guide for LAs -->
      <xs:element name="SENtypeRank" type="sentyperanktype" minOccurs="0" maxOccurs="1"/>
</xs:sequence>
</xs:complexType>

<!-- Types defined below -->

    <xs:simpleType name="yesnorecruittype">
    <xs:restriction base="nonEmptyString">
	    <xs:enumeration value="Y"><xs:annotation><xs:documentation>Yes</xs:documentation></xs:annotation></xs:enumeration>
	    <xs:enumeration value="N"><xs:annotation><xs:documentation>No</xs:documentation></xs:annotation></xs:enumeration>
      <xs:enumeration value="R"><xs:annotation><xs:documentation>Recruitment in progress</xs:documentation></xs:annotation></xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="ulntype">
    <xs:restriction base="xs:string">
      <xs:pattern value="\d{10}"/>
    </xs:restriction>
  </xs:simpleType>

<!-- Module 1 starts here -->
  <xs:simpleType name="sextype">
    <xs:restriction base="nonEmptyString">
      <xs:enumeration value="M"><xs:annotation><xs:documentation>Male</xs:documentation></xs:annotation></xs:enumeration>
      <xs:enumeration value="F"><xs:annotation><xs:documentation>Female</xs:documentation></xs:annotation></xs:enumeration>
    </xs:restriction>
  </xs:simpleType>

