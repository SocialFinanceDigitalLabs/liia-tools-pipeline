    <xs:pattern value="^\d{1,6}$"/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="ukprntype">
  <xs:restriction base="xs:string">
  <!-- Below says has to be exactly 8 characters long -->
    <xs:pattern value="^\d{8}$"/>
  </xs:restriction>
</xs:simpleType>

