  <xs:simpleType name="upntype">
    <xs:restriction base="xs:string">
    <!-- We need to have a 13 character format Annnnnnnnnnnn with AnnnnnnnnnnnA for a temp UPN where A is a character and n is numeric. Further info available in UPN policy and practice guidance -->
        <!-- SC's doc has the following: <xs:pattern value="[A-Za-z]\d{11}(\d|[A-Za-z])"/> -->
        <xs:pattern value="([A-Za-z]\d{12}|[A-Za-z]\d{11}[A-Za-z])"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="uniquelearnernumbertype">
    <xs:restriction base="xs:string">
    <!-- Used link in guide for LAs and saw pattern for Learner Reference Number is [A-Za-z0-9 ]{1,12} (any combination of up to 12 alphabetic characters, numeric digits or spaces) -->
      <!-- SC has the following: <xs:pattern value="\d{10}"/> -->
      <xs:pattern value="[A-Za-z0-9]{1,12}"/>
    </xs:restriction>
  </xs:simpleType>

